package org.oswxf.skinui.events
{
	public class ViewBlockEvent extends SCEvent
	{
		/**
		 * 休眠完成
		 * 所有动态效果全部停止 
		 */
		static public const SLEEPED:String = 'sleeped';
		/**
		 * 启动完成
		 * 所有动态效果开始播放 
		 */
		static public const STARTED:String = 'started';
		
		/**
		 * 与初始化 
		 */		
		static public const PRE_INIT:String = 'pre_init';
		/**
		 * 初始化完成
		 * 所有bundler可用 
		 */
		static public const INIRED:String = 'inited';
		/**
		 * 创建完成 
		 */
		static public const CREATED:String ='creat_complete';
		public function ViewBlockEvent(type:String)
		{
			super(type);
		}
	}
}