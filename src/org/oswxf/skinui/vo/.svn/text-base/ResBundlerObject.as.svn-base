package org.oswxf.skinui.vo
{
	import flash.utils.ByteArray;
	import flash.utils.Dictionary;

	/**
	 * 资源包定义 
	 * @author nativeas
	 * 
	 */
	public class ResBundlerObject
	{
		public function ResBundlerObject()
		{
		}
		
		/**
		 * URI，资源定位符
		 * 唯一标识，不会修改 
		 */		
		public var uri:String = '';
		
		/**
		 *资源包来源地址
		 */
		public var url:String = '';
		
		/**
		 * 资源包注释 
		 */		
		public var desc:String = "";
		
		/**
		 * 二进制数据 
		 */
		public var binary:ByteArray = null;
		
		/**
		 * 元件字典 
		 */		
		public var movieDic:Dictionary;
	}
}